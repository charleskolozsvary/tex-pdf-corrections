	\iffalse	\underline{When (i) and (iii) hold}, for\marks\segmentmark{5384} any\marks\segmentmark{5385} basis\marks\segmentmark{5386} element\marks\segmentmark{5387} $v_q \in H_{2i}^{\mathbb{C}G}$, the\marks\segmentmark{5388} coefficient\marks\segmentmark{5389} of\marks\segmentmark{5390} $v_p$ in\marks\segmentmark{5391} $\tilde f_*(v_q)$, , is\marks\segmentmark{5392} 
	\begin{equation}\label{i,ii,1}
		\langle \delta_{v_p}, \tilde f_*v_q\rangle
		= \langle \tilde f^* \delta_{v_p}, v_q\rangle
		= \langle \lambda^i \delta_{v_p}, v_q\rangle
		= \lambda^i \delta_{pq},
	\end{equation}
	and\marks\segmentmark{5393} the\marks\segmentmark{5394} coefficient\marks\segmentmark{5395} of\marks\segmentmark{5396} $\vartheta\otimes v_p \in \vartheta\otimes H_{2i}^{\mathbb{C}G}$ in\marks\segmentmark{5397} $\tilde f_*(v_q)$ is\marks\segmentmark{5398} 
	\begin{equation}\label{1,iii,2}
